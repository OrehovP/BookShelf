@model IEnumerable<LogicLayer.PresentationModel.PGenre>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetGenres</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
</head>
<body class="m-0 p-0">
    <div class="container-fluid w-75 mt-5">
        <ul class="list-group list-group-flush">
            @foreach (var item in Model)
            {
                <li class="list-group-item">@Html.DisplayFor(modelItem => item.Name)</li>
            }
        </ul>
        @Html.ActionLink("Вернуться назад", "Index", null, new { @class = "btn btn-primary" })
    </div>
</body>
</html>
